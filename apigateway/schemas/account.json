{
    "$schema": "http://json-schema.org/schema#",
    "id": "http://schema.fathomai.com/schemas/account.json",
    "description": "Account",
    "type": "object",
    "properties": {
        "id": {
            "description": "The account id",
            "type": "string",
            "readonly": true
        },
        "active": {
            "type": "bool"
        },
        "campaigns": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "code": {
            "description": "The access code",
            "type": "string"
        },
        "conference": {
            "type": "string"
        },
        "created_date": {
            "description": "The date the account was created",
            "type": "string",
            "readonly": true
        },
        "division": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "personas": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "seats": {
            "type": "number"
        },
        "status": {
            "type": "string"
        },
        "users": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "The users in the account",
            "readonly": true
        },
        "updated_date": {
            "description": "The date the user was last updated",
            "type": "string"
        }
    },
    "required": [
        "seats",
        "name"
    ],
    "additionalProperties": false
}
