{
    "$schema": "http://json-schema.org/schema#",
    "id": "http://schema.fathomai.com/schemas/user.json",
    "description": "User",
    "type": "object",
    "properties": {
        "email": {
            "description": "User's email address",
            "type": "string",
            "readonly": true
        },
        "role": {
            "description": "Role",
            "enum": [
                "admin" ,
                "athlete"
            ]
        },
        "password": {
            "type": "string",
            "readonly": true
        },
        "email_verified": {
            "enum": ["true", "false"]
        }
    },
    "required": [
        "email",
        "password"
    ],
    "additionalProperties": false
}
